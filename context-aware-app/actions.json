[
  {
    "action": {
      "template": "${0}",
      "type": "url"
    },
    "meta": {
      "id": "builtin-url",
      "name": "打开网址",
      "version": "1.0.0"
    },
    "scope": {
      "include": [
        "*"
      ],
      "priority": 90
    },
    "trigger": {
      "extraction_pattern": "(https?://|www\\.)[\\x21-\\x7e]+",
      "pattern": "^.*(https?:\\/\\/|www\\.)([\\w_-]+(?:(?:\\.[\\w_-]+)+))([\\w.,@?^=%&:/~+#-]*[\\w@?^=%&/~+#-])?",
      "type": "regex"
    }
  },
  {
    "action": {
      "template": "",
      "type": "doi_scihub"
    },
    "meta": {
      "id": "builtin-doi",
      "name": "打开DOI",
      "version": "1.0.0"
    },
    "scope": {
      "include": [
        "*"
      ],
      "priority": 90
    },
    "trigger": {
      "extraction_pattern": "10\\.\\d{4,9}/[-._;()/:a-zA-Z0-9]+",
      "pattern": "\\b10\\.\\d{4,9}/[-._;()/:a-zA-Z0-9]+",
      "type": "regex"
    }
  },
  {
    "action": {
      "template": "${0}",
      "type": "path"
    },
    "meta": {
      "id": "builtin-path-windows",
      "name": "打开文件路径",
      "version": "1.0.0"
    },
    "scope": {
      "include": [
        "*"
      ],
      "priority": 90
    },
    "trigger": {
      "extraction_pattern": "[a-zA-Z]:\\\\(?:[^\\\\/:*?\"<>|\\r\\n]+\\\\)*[^\\\\/:*?\"<>|\\r\\n]*",
      "pattern": "^[a-zA-Z]:\\\\(?:[^\\\\/:*?\"<>|\\r\\n]+\\\\)*[^\\\\/:*?\"<>|\\r\\n]*$",
      "type": "regex"
    }
  },
  {
    "action": {
      "template": "",
